{
  "compilerOptions": {
    "target": "es5",                                    
    "module": "commonjs",           
    "lib": ["ES2015","dom"],
    "sourceMap": true,
    "outDir": "./script",                    
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "alwaysStrict": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
  }
}
